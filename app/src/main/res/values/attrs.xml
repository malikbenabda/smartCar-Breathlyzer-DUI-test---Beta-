<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="ProgressCircle_android_textColor"></declare-styleable>
    <declare-styleable name="ProgressCircle_android_textSize"></declare-styleable>
    <declare-styleable name="ProgressCircle_strokeWidth"></declare-styleable>
    <declare-styleable name="ProgressCircle_progressColor"></declare-styleable>
    <declare-styleable name="ProgressCircle_incompleteProgressColor"></declare-styleable>
    <declare-styleable name="ProgressCircle"></declare-styleable>
    <declare-styleable name="CircleProgress">
        <attr name="circle_progress" format="integer"/>
        <attr name="circle_max" format="integer"/>

        <attr name="circle_unfinished_color" format="color"/>
        <attr name="circle_finished_color" format="color"/>

        <attr name="circle_text_size" format="dimension"/>
        <attr name="circle_text_color" format="color"/>

        <attr name="circle_prefix_text" format="string"/>
        <attr name="circle_suffix_text" format="string"/>
    </declare-styleable>
    <declare-styleable name="Themes">
        <attr name="circleProgressStyle" format="reference"/>
    </declare-styleable>
</resources>